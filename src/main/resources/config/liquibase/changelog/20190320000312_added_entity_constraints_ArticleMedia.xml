<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity ArticleMedia.
    -->
    <changeSet id="20190320000312-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="article_id"
                                 baseTableName="article_media"
                                 constraintName="fk_article_media_article_id"
                                 referencedColumnNames="id"
                                 referencedTableName="article"/>

        <addForeignKeyConstraint baseColumnNames="vibe_user_id"
                                 baseTableName="article_media"
                                 constraintName="fk_article_media_vibe_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vibe_user"/>

    </changeSet>
</databaseChangeLog>
